# When set, OpenWeather is the primary provider with Open-Meteo as fallback.
# When empty, Open-Meteo runs standalone (no API key required).
OPENWEATHER_API_KEY=
APP_ACCESS_TOKEN=
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=60
CACHE_TTL_SECONDS=300
NEXT_PUBLIC_DEFAULT_TEMPERATURE_UNIT=metric
