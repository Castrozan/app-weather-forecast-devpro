# Open-Meteo is used as the primary provider and does not require an API key.
# When OPENWEATHER_API_KEY is set, OpenWeather is used as a fallback if Open-Meteo fails.
OPENWEATHER_API_KEY=
APP_ACCESS_TOKEN=
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=60
CACHE_TTL_SECONDS=300
NEXT_PUBLIC_DEFAULT_TEMPERATURE_UNIT=metric
